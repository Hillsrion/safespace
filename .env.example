# API Keys (Required to enable respective provider)
ANTHROPIC_API_KEY=your_anthropic_api_key_here       # Required: Format: sk-ant-api03-...
PERPLEXITY_API_KEY=your_perplexity_api_key_here     # Optional: Format: pplx-...
OPENAI_API_KEY=your_openai_api_key_here             # Optional, for OpenAI/OpenRouter models. Format: sk-proj-...
GOOGLE_API_KEY=your_google_api_key_here             # Optional, for Google Gemini models.
MISTRAL_API_KEY=your_mistral_key_here               # Optional, for Mistral AI models.
XAI_API_KEY=YOUR_XAI_KEY_HERE                       # Optional, for xAI AI models.
AZURE_OPENAI_API_KEY=your_azure_key_here            # Optional, for Azure OpenAI models (requires endpoint in .taskmasterconfig).

# Session secret (Required)
SESSION_SECRET=your_session_secret_here

# Super Admin Configuration (Required for seed script)
SUPERADMIN_EMAIL=admin@example.com
SUPERADMIN_PASSWORD=password
SUPERADMIN_FIRSTNAME=Admin
SUPERADMIN_LASTNAME=User

# Seed Configuration (Optional - defaults are used if not set)
SEED_NUM_USERS=20
SEED_NUM_CITIES=5
SEED_SPACES_PER_CITY_MIN=7
SEED_SPACES_PER_CITY_MAX=10
SEED_POSTS_PER_SPACE_MIN=18
SEED_POSTS_PER_SPACE_MAX=22
SEED_ADDITIONAL_POSTS_PER_USER=3
SEED_USERS_PER_SPACE_MIN=3
SEED_USERS_PER_SPACE_MAX=7


# Database Configuration (Required)
DATABASE_URL=postgresql://postgres@localhost:5432/safespace